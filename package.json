{
  "name": "mnchromeext",
  "version": "1.0.0",
  "type": "module",
  "description": "A template project for building Chrome extensions using Manifest V3.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "npx tailwindcss -i src/styles/tailwind.css -o popup.css --minify --postcss",
    "build": "node build.js",
    "build:local": "npm run build:css && node build.js",
    "watch": "node build.js --watch",
    "db:push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdjs/MNChromeExt.git"
  },
