  },
  "homepage": "https://github.com/xdjs/MNChromeExt#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.30.8",
    "express": "^5.1.0",
    "openai": "^5.10.1",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.0",
    "@types/node": "^20.19.8",
    "@vercel/node": "^2.3.0",
